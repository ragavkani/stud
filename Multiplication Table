import java.io.*;
import java.util.*;
class Mul
{
public static void main(String args[])
{ 
int a,i,j;
Scanner sc=new Scanner(System.in);
System.out.println("Enter the Table Number:");
a=sc.nextInt();
for(i=a;i<=a;i++)
{
for(j=1;j<=10;j++)
{
System.out.println(i +"x" +j +"="+i*j);

}
System.out.println("  ");
}
}
}
