import java.io.*;
import java.util.*;
class ArmstrongNumber
{
public static void main(String args[])
{
int n,r,sum=0,s;
Scanner sc=new Scanner(System.in);
System.out.println("Enter a Number");
n=sc.nextInt();
s=n;
while(n>0)
{
r=n%10;
sum+=Math.pow(r,3);
n=n/10;
}
if(s==sum)
System.out.println("Armstrong Number");
else
System.out.println("Not Armstrong number");
}
}
