import java.io.*;
import java.util.*;
class ReverseNumber
{
public static void main(String args[])
{
int n,r,sum=0,s;
Scanner sc=new Scanner(System.in);
System.out.println("Enter a Number");
n=sc.nextInt();
s=n;
while(n>0)
{
r=n%10;
sum=sum*10+r;
n=n/10;
}

System.out.println("The Reverse of "+s+" = " +sum);
}
}
