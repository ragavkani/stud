import java.io.*;
import java.util.*;
class SumofDigit
{
public static void main(String args[])
{
int n,r,sum=0,s;
Scanner sc=new Scanner(System.in);
System.out.println("Enter a Number");
n=sc.nextInt();
s=n;
while(n>0)
{
r=n%10;
sum+=r;
n=n/10;
}

System.out.println("The Sum of digit is:  "+sum);
}
}
